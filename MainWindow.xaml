<Window x:Class="polymorphismExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:polymorphismExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="btnExample1" Click="btnExample1_Click">Example 1</Button>
        <Button x:Name="btnExample2" Click="btnExample2_Click">Example 2</Button>
        <Button x:Name="btnExample3" Click="btnExample3_Click">Example 3</Button>
        <Label x:Name="lblOutput"></Label>
        
    </StackPanel>
</Window>
